#!/bin/csh

gnuplot << EOF
set output "bsa_combine_cont.eps"
set terminal postscript enhanced color
set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"
set xlabel "Time [UT]"
set ylabel "Contention C_C"
set title "Contention profile C_C during night of 14-15 Oct 2007 under variable environment conditions"
set yrange [0:30]
set format x "%H"
plot "bsa1_pr_cont.dat"   using 1:3 with line ti "E_{FP}", \
     "bsa7_ex_cont.dat"   using 1:3 with line ti "E_{FX}", \
     "bsa13_rnd_cont.dat" using 1:3 with line ti "E_{RA}", \
     "bsa14_rnd_cont.dat" using 1:3 with line ti "E_{RA}"
EOF
