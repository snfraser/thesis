#!/bin/csh

gnuplot << EOF
set output "bsb_combine_cont.eps"
set terminal postscript enhanced color
set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"
set xlabel "Time [UT]"
set ylabel "Contention C_C"
set title "Contention profile C_C during night of 7-8 Nov 2007 under variable environment conditions"
set yrange [0:60]
set format x "%H"
plot "bsb1_pr_cont.dat"   using 1:3 with line ti "E_{FP}", \
     "bsb7_ex_cont.dat"   using 1:3 with line ti "E_{FX}", \
     "bsb13_rnd_cont.dat" using 1:3 with line ti "E_{RA}", \
     "bsb14_rnd_cont.dat" using 1:3 with line ti "E_{RA}"
     
EOF
