#!/bin/csh

gnuplot << EOF
set output "bsb_rnd1_cont.eps"
set terminal postscript enhanced color
set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"
set xlabel "Time [UT]"
set ylabel "Contention C_C"
set title "Contention profile C_C during night of 7-8 Nov 2007 under conditions E_{RA}"
set yrange [0:60]
set format x "%H"
plot "bsb13_rnd_cont.dat" using 1:3 with line ti "Run 1", \
     "bsb14_rnd_cont.dat" using 1:3 with line ti "Run 2", \
     "bsb15_rnd_cont.dat" using 1:3 with line ti "Run 3"
EOF
