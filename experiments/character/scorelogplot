#!/bin/csh

/usr/bin/gnuplot -persist << EOF

set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"

set title "Variation of score for winning candidate with time"
set xlabel "Time"
set ylabel "Winning score  f_{SU}"
set format x "%H:%M"
set output "winscore.eps"
set term postscript eps enhanced color
plot "scorelog3" using 1:3 wi steps ti "Winning score" , "scorelog3" using 1:3 wi po pt 5 ps 1 ti "", "bb3.txt" using 1:3 wi li ti "2494J000t000"

EOF
