#!/bin/csh

# setup simulation name
if (${#argv} < 1) then
    echo "No bleeding name supplied"
    exit 1
endif

set SIM = $1
set simlog = sim_${SIM}.log
 
echo "Extract from $simlog "

cat $simlog | grep CDAT_NGS   | awk '{print $12,$13,$14}' > ${SIM}_ng.dat
cat $simlog | grep CDAT_CC_AV | awk '{print $12,$13,$14}' > ${SIM}_cav.dat
cat $simlog | grep CDAT_CC_T  | awk '{print $12,$13,$14}' > ${SIM}_tt.dat

cat $simlog | grep "CDAT_YT.*CDAT_XT" | awk '{print $12,$13,$14}' > ${SIM}_xt.dat
