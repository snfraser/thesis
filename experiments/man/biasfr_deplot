#!/bin/csh

#/usr/bin/gnuplot -persist << EOF
/usr/bin/gnuplot  << EOF

set output "biasfr_de.eps"
set term postscript enhanced color

set title "Variation of average Q_{SU} with environmental stability parameter {/Symbol D}_E"
set xlabel "Environmental stability {/Symbol D}_E [mins]"
set ylabel "Q_{SU}"

set label "<- Erratic" at graph 0.05,0.05 tc lt 1
set label "Stable ->" at graph 0.85,0.05 tc lt 1

set boxwidth 2
set style fill solid

plot "biasfr_de.dat" using 1:2:3:4:5 with candlesticks notitle

EOF
