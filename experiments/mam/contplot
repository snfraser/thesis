#!/bin/csh

/usr/bin/gnuplot << EOF
set xdata time
set timefmt "%Y-%m-%d %H:%M"

set key top left
set title "Contention C_C for night 13-14 Nov 2007"
set xlabel "Time"
set ylabel "C_C"
set format x "%H:%M"

set output "cont.eps"
set term postscript eps enhanced color

plot "cont.dat" using 1:3 ps 0.5 pt 7 ti "Predicted","cand_cont.dat" using 1:3 ps 0.5 pt 7 ti "Actual"

EOF
