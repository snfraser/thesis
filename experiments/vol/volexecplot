#!/bin/csh

/usr/bin/gnuplot -persist << EOF

set title "Rate of increase of executable observations"
set xlabel "Date [2010]"
set ylabel "Change per day [mins]"

unset key
set xdata time
set timefmt "%Y-%m-%d"
set format x "%b"

set output "volexec.eps"
set term postscript eps enhanced color
plot "vol_plot.dat" using 1:3 with lines 

EOF
