#!/bin/csh

/usr/bin/gnuplot -persist << EOF
set style data histogram
set boxwidth 10.0 absolute
set style fill solid 1.00 border -1 
set label "Average: change= 5.2 mins" at screen 0.65, screen 0.75
set xrange [-10:600]
set title "Distribution of values of execution time for received updates"
set xlabel "Execution time [minutes]"
set ylabel "Number of updates" 
unset key

set term postscript eps enhanced color
set output "vol_xt.eps"
plot "vol_xt.dat" using 1:3 wi boxes
EOF
